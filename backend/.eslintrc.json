{
    "env": {
      "node": true,
      "es2021": true
    },
    "extends": ["eslint:recommended", "plugin:prettier/recommended"],
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module"
    },
    "rules": {
      "no-unused-vars": ["error"],
      "prettier/prettier": ["error"],
      "semi": ["error", "always"],
      "quotes": ["error", "single"]
    }
  }
  